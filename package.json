{
  "name": "@qwestify/test-utils",
  "version": "3.19.2-patch.1",
  "description": "Questify-patched Nuxt Test Utilities (stable fork of @nuxt/test-utils)",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/12529572-Canada-Inc/test-utils.git"
  },
  "exports": {
    ".": "./dist/e2e.mjs",
    "./config": "./dist/config.mjs",
    "./e2e": "./dist/e2e.mjs",
    "./playwright": "./dist/playwright.mjs",
    "./experimental": "./dist/experimental.mjs",
    "./module": "./dist/module.mjs",
    "./runtime": "./dist/runtime-utils/index.mjs",
    "./vitest-environment": "./dist/vitest-environment.mjs"
  },
  "typesVersions": {
    "*": {
      ".": ["./dist/e2e.d.mts"],
      "config": ["./dist/config.d.mts"],
      "e2e": ["./dist/e2e.d.mts"],
      "playwright": ["./dist/playwright.d.mts"],
      "experimental": ["./dist/experimental.d.mts"],
      "module": ["./dist/module.d.mts"],
      "runtime": ["./dist/runtime-utils/index.d.mts"],
      "vitest-environment": ["./dist/vitest-environment.d.mts"]
    }
  },
  "files": [
    "dist",
    "dirs.js",
    "README.md"
  ],
  "scripts": {
    "prepare": "pnpm build",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --run",
    "prepack": "unbuild",
    "postinstall": "echo 'Skipping example builds (forked @qwestify/test-utils)'"
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.3",
    "c12": "^3.3.0",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "destr": "^2.0.5",
    "estree-walker": "^3.0.3",
    "fake-indexeddb": "^6.2.2",
    "get-port-please": "^3.2.0",
    "h3": "^1.15.4",
    "local-pkg": "^1.1.2",
    "magic-string": "^0.30.19",
    "node-fetch-native": "^1.6.7",
    "node-mock-http": "^1.0.3",
    "ofetch": "^1.4.1",
    "pathe": "^2.0.3",
    "perfect-debounce": "^2.0.0",
    "radix3": "^1.1.2",
    "scule": "^1.3.0",
    "std-env": "^3.9.0",
    "tinyexec": "^1.0.1",
    "ufo": "^1.6.1",
    "unplugin": "^2.3.10",
    "vitest-environment-nuxt": "^1.0.1",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^3.2.4",
    "eslint": "^9.37.0",
    "vue-tsc": "^3.1.1"
  },
  "peerDependencies": {
    "@playwright/test": "^1.43.1",
    "@testing-library/vue": "^8.0.1",
    "@vue/test-utils": "^2.4.6",
    "vitest": "^3.2.0",
    "happy-dom": "^19.0.0",
    "jsdom": "^27.0.0"
  },
  "peerDependenciesMeta": {
    "@playwright/test": { "optional": true },
    "@testing-library/vue": { "optional": true },
    "@vue/test-utils": { "optional": true },
    "vitest": { "optional": true },
    "happy-dom": { "optional": true },
    "jsdom": { "optional": true }
  },
  "resolutions": {
    "@nuxt/kit": "^4.1.3",
    "@nuxt/schema": "^4.1.3",
    "vite": "7.1.9",
    "vitest": "3.2.4",
    "vue": "^3.5.22"
  },
  "engines": {
    "node": "^20 || ^22 || >=24"
  },
  "packageManager": "pnpm@10.18.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "better-sqlite3"
    ],
    "ignoredBuiltDependencies": [
      "esbuild",
      "oxc-resolver",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
